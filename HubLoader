nouni=false
local function lo(l)
loadstring(game:HttpGet(l,true))()
nouni=true
end

if game.PlaceId==4390380541 then --Rumble Quest
lo("https://raw.githubusercontent.com/MaxGatHub/WechugeHub/main/RumbleQuest")
end

if nouni==false then --Universal
lo("https://raw.githubusercontent.com/MaxGatHub/WechugeHub/main/Universal")
end
